{% for product in section.settings.featured_collection.products %}
    {% render 'product-card', product: product %}
{% endfor %}

{% schema %}
{
    "name": "Featured Collection",
    "settings": [
        {
            "type": "text",
            "id": "title",
            "default": "Feaqtured Collection",
            "label": "title"
        },
        {
            "type": "collection",
            "id": "featured_collection",
            "label": "Collection"
        }
    ]
}
{% endschema %}